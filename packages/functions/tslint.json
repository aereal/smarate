{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": [
            true,
            {
                "jsxBracketSameLine": false,
                "trailingComma": "es5",
                "semi": false
            }
        ],
        "interface-name": [true, "never-prefix"]
    },
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ]
}
